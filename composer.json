    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "7.*",
        "illuminate/filesystem": "^v8.40",
        "illuminate/translation": "^v8.40",
        "illuminate/validation": "^v8.40",
        "vlucas/phpdotenv": "^5.3.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Breno S. Grillo",
            "email": "brenogrillo@gmail.com",
            "homepage": "https://github.com/bsampaio",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Lifepet\\Wallet\\SDK\\": "src"
